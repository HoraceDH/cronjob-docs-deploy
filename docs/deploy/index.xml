&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl"> &lt;span class="kn">listen&lt;/span> &lt;span class="mi">80&lt;/span>&lt;span class="p">;&lt;/span>
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl"> &lt;span class="kn">server_name&lt;/span> &lt;span class="s">demo.cronjob.horace.cn&lt;/span> &lt;span class="mi">172&lt;/span>&lt;span class="s">.16.62.4&lt;/span>&lt;span class="p">;&lt;/span>
